// Place your key bindings in this file to overwrite the defaults
[
    { "key": "ctrl+y",          "command": "editor.action.deleteLines"},
    { "key": "alt+left",        "command": "workbench.action.previousEditor" },
    { "key": "alt+right",       "command": "workbench.action.nextEditor" },

    { "key": "ctrl+1",          "command": "workbench.view.explorer" },

    // Debugging
    { "key": "f5",              "command": "editor.debug.action.toggleBreakpoint", 
                                   "when": "editorTextFocus" },
    { "key": "f7",              "command": "workbench.action.debug.stepInto",
                                   "when": "inDebugMode" },
    { "key": "f8",              "command": "workbench.action.debug.stepOver",
                                   "when": "inDebugMode" },
    { "key": "shift+f8",        "command": "workbench.action.debug.stepOut",
                                   "when": "inDebugMode" },
    { "key": "shift+f9",        "command": "workbench.action.debug.start",
                                   "when": "!inDebugMode" },
    { "key": "ctrl+f9",         "command": "workbench.action.debug.continue",      
                                   "when": "inDebugMode" }

]
